
answer = 0
chut = 1

for x in range(999,1,-1):
    for y in range(999,1,-1):
        s = str(x*y)
        chut = 1
        for a in range(0,int(len(s)/2)):
            if(s[a]!=(s[-a-1])):
                chut = -1
                break
        if(chut==1):
            if(x*y>answer): answer = x*y
            print(str(x)+" "+str(y)+" "+str(x*y))
print(answer)


